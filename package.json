{
    "name": "woo-category-grid-loader",
    "version": "0.1.0-beta.1",
    "description": "WooCommerce Category Grid Loader WordPress Plugin",
    "main": "index.js",
    "scripts": {
        "version": "npm run version-update && git add composer.json woo-category-grid-loader.php",
